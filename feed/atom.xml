---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
  <title>Litefy's Blog 偶然 自省 生活</title>
  <link href="http://litefy.github.com/blog/"/>
  <link type="application/atom+xml" rel="self" href="http://litefy.github.com/blog/atom.xml"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://litefy.github.com/blog/</id>
  <author>
    <name>Litefy</name>
    <email>litefy@gmail.com</email>
  </author>


 {% for post in site.posts %}
  <entry>
    <id>{{ site.url }}{{ post.id }}</id>
    <link type="text/html" rel="alternate" href="{{ site.url }}{{ post.url }}"/>
    <title>{{ post.title | xml_escape }}</title>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Litefy</name>
      <uri>http://litefy.github.com/blog/</uri>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
 {% endfor %}

</feed>